    "bracketSpacing": false,
    "proseWrap": "never",
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "gulp": "./node_modules/gulp/bin/gulp.js",
    "start": "gulp",
    "prettier": "prettier --write \"**/*.{js,json,md,css}\"",
    "precommit": "lint-staged"
  },
  "spm": {
    "main": "./animate.css"
  }
}